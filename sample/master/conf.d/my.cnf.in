[client]                           
port=3306
socket=/var/run/mysqld/mysqld.sock

[mysqld]                           
port=3306
socket=/var/run/mysqld/mysqld.sock
key_buffer_size=16M
max_allowed_packet=8M
character-set-server=utf8

server-id               = SERVERID
log_bin                 = /var/log/mysql/mysql-bin.log

[mysqldump]                        
quick

[mysqld_safe]                      
#log-error=/var/log/mysqld.log
#pid-file=/var/run/mysqld/mysqld.pid





